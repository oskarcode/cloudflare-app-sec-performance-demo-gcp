{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"lib": ["ES2020"],
		"moduleResolution": "bundler",
		"types": ["@cloudflare/workers-types"],
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": false,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true
	},
	"include": ["**/*.ts"],
	"exclude": ["node_modules", ".wrangler"]
}
